<div id="frases">
    <div class="carrousel">
        {% for index, row in rows | slice(0,3) %}
            {% set row_classes = [
            default_row_class ? 'views-row',
            'panel',
            'panel-default',
            ] %}

            <div class="frases-item col-sm-4">
                {{ row.content }}
            </div>
        {% endfor %}
    </div>
    <div class="frases-list">
        <div class="frases-items">
            {% for index, row in rows | slice(3,4) %}
                {% set row_classes = [
                default_row_class ? 'views-row',
                'panel',
                'panel-default',
                ] %}

                <div class="frases-item col-xs-12 col-sm-6">
                    {{ row.content }}
                </div>
            {% endfor %}
        </div>
    </div>
</div>