<div>
    <div class="pleca-seccion">
        <span class="category-nombres"></span><a href="/nombres" class="text-upper">Nombres</a>
    </div>
    <div class="nombres-content">
        {% for index, row in rows | slice(1,3) %}
            {% set row_classes = [
                default_row_class ? 'views-row',
                'panel',
                'panel-default',
            ] %}

            <div class="col-xs-12 col-sm-6 col-md-4">
                {{ row.content }}
            </div>
        {% endfor %}
        <div class="clear"></div>
    </div>
    <div class="clear"></div>

    <div class="nombres-glosary">
        <h3 class="text-center text-black">Busca nombres por letra</h3>
        {{ drupal_view('home', 'nombres_indice') }}
    </div>
    <div class="clear"></div>
    <div class="nombres-content">
        <h2 class="text-center text-black more-names">M&aacute;s nombres</h2>
        {% for index, row in rows | slice(4,3) %}
            {% set row_classes = [
                default_row_class ? 'views-row',
                'panel',
                'panel-default',
            ] %}

            <div class="col-sm-4">
                {{ row.content }}
            </div>
        {% endfor %}
        <div class="clear"></div>
    </div>

</div>
