<div id="last-news">
  {% for index, row in rows | slice(1,3) %}
    {%
    set row_classes = [
    default_row_class ? 'views-row',
    'panel',
    'panel-default',
    ]
    %}
    <div class="nodeListItem col-md-4 col-sm-6 col-xs-12">
      <div class="nodeListItemInner">
        {{ row.content }}
      </div>
    </div>
  {% endfor %}
  <div class="clear"></div>
</div>

