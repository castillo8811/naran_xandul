<div id="last-news">
  {% for index, row in rows %}
    {%
    set row_classes = [
    default_row_class ? 'views-row',
    'panel',
    'panel-default',
    ]
    %}

    {% if index==4 %}
      {{ drupal_view('home', 'cursos') }}
    {% endif  %}

    {% if index==7 %}
      {{ drupal_view('home', 'nombres') }}
    {% endif  %}

    {% if index==9 %}
      {{ drupal_view('home', 'frases') }}
    {% endif  %}

    <div class="nodeListItem col-md-4 col-sm-6 col-xs-12">
      <div class="nodeListItemInner">
        {{ row.content }}
      </div>
    </div>
  {% endfor %}
</div>